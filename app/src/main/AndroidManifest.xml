<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cyprian.money">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MoneyManager.NoActionBar">
        <activity android:name="com.cyprian.money.RegisterActivity"></activity>
        <activity android:name="com.cyprian.money.SplashActivity" />
        <activity
            android:name="com.cyprian.money.LoginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.cyprian.money.AddCategoryActivity" />
        <activity android:name="com.cyprian.money.MainActivity" />
        <activity android:name="com.cyprian.money.IncomeExpenseActivity" />
        <activity android:name="com.cyprian.money.CategoryActivity" />
        <activity android:name="com.cyprian.money.IncomeActivity" />
        <activity
            android:name="com.cyprian.money.DashboardActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.MoneyManager.NoActionBar" />
    </application>

</manifest>